      <DTS:PropertyExpression
        DTS:Name="[Get SSISDB Data Using Dynamic SQL Experssion, ADO NET Source].[SqlCommand]">"SELECT [Event Message ID] = EM.event_message_id
	,[Message Time] = EM.message_time
	,[Message] = EM.[message]
	,[Package Name] = EM.package_name
	,[Event Name] = EM.event_name
	,[Message Source Name] = EM.message_source_name
	,[Package Path] = EM.package_path
	,[Execution Path] = EM.execution_path
	,[Message Type] = EM.message_type
	,[Message Source Type] = EM.message_source_type
FROM SSISDB.CATALOG.event_messages EM
WHERE EM.package_name = '" + @[$Project::PackageName] + "'" + "
	AND EM.message_time &gt; DATEADD(dd, " + (DT_STR, 4, 1252)@[$Project::Days] + ", current_timestamp)
	AND EM.event_name NOT LIKE '%Validate%'
ORDER BY EM.message_time DESC;"</DTS:PropertyExpression>
